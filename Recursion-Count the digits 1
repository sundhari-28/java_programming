import java.util.*;
public class Main {
    static int count(int n) {
        if (n < 0)
            n = -n;
        if (n == 0)
            return 0;
        else
            return 1 + count(n / 10);
    }
public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        if (num == 0)
            System.out.println("The Count the digits: ");
        else
            System.out.println("The Count the digits: " + count(num));
    }
}
