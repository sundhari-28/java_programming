import java.util.*;
public class Main{
    public static void main(String []args){
        Scanner sc=new  Scanner(System.in);
        int num=sc.nextInt();
        int a=num;
        int sum=0;
        while(a>0){
            sum+=a%10;
            a=a/10;
        }
        if(num%sum==0){
            System.out.println("Harshad Number");
        }
        else{
            System.out.println("Not Harshad Number");
        }
    }
}
